{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "yournamespace/parent",
	"version": "0.1.0",
	"title": "Parent",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"variations": [
		{
			"name": "parent-variation-left",
			"title": "Parent Variation Left",
			"attributes": {
				"variant": "left"
			},
			"innerBlocks": [
				[
					"yournamespace/child-static",
					{
						"content": "I am a child static left"
					}
				]
			],
			"isActive": ["variant"],
			"isDefault": true
		},
		{
			"name": "parent-variation-right",
			"title": "Parent Variation Right",
			"attributes": {
				"variant": "right"
			},
			"innerBlocks": [
				[
					"yournamespace/child-static",
					{
						"content": "I am a child static right"
					}
				]
			],
			"isActive": ["variant"],
			"scope": ["block"]
		}
	],
	"attributes": {
		"variant": {
			"type": "string"
		}
	},
	"parent": ["yournamespace/parent-container"],
	"textdomain": "yourtextdomain",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
