{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "yournamespace/container",
	"version": "0.1.0",
	"title": "Container",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": true,
		"anchor": true,
		"color": {
			"text": true,
			"background": true,
			"gradients": true,
			"__experimentalDefaultControls": {
				"color": false,
				"background": false,
				"gradients": false
			}
		},
		"spacing": {
			"padding": true,
			"__experimentalDefaultControls": {
				"padding": false
			}
		}
	},
	"attributes": {
		"position": {
			"enum": ["static", "relative"],
			"default": "static"
		},
		"isolation": {
			"enum": ["isolate", "auto"],
			"default": "auto"
		},
		"textAlign": {
			"enum": ["left", "center", "right"],
			"default": "left"
		},
		"hasOutline": {
			"type": "boolean",
			"default": true
		}
	},
	"textdomain": "yourtextdomain",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
