{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "yournamespace/provider",
	"version": "0.1.0",
	"title": "Provider",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"usesContext": ["postId", "postType"],
	"attributes": {
		"message": {
			"type": "string",
			"default": "Hello from Provider block!"
		}
	},
	"providesContext": {
		"myCusomPlugin/message": "message"
	},
	"textdomain": "yourtextdomain",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
