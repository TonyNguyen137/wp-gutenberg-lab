{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "yournamespace/image",
	"version": "0.1.0",
	"title": "Bild",
	"category": "media",
	"icon": "format-image",
	"description": "",
	"example": {
		"attributes": {
			"url": "https://picsum.photos/700/500"
		}
	},
	"supports": {
		"html": false
	},
	"attributes": {
		"id": {
			"type": "number"
		},
		"url": {
			"type": "string"
		},
		"alt": {
			"type": "string"
		},
		"width": {
			"type": "number"
		},
		"height": {
			"type": "number"
		},

		"loading": {
			"type": "string",
			"enum": ["auto", "lazy", "eager"],
			"default": "auto"
		},

		"srcSize": {
			"type": "string",
			"default": ""
		},
		"srcsetSizes": {
			"type": "array",
			"default": []
		},
		"sizes": {
			"type": "string"
		},
		"fetchPriority": {
			"type": "string",
			"enum": ["auto", "high", "low"],
			"default": "auto"
		},
		"isFullWidth": {
			"type": "boolean",
			"default": false
		},
		"isFullHeight": {
			"type": "boolean",
			"default": false
		},
		"aspectRatio": {
			"type": "string"
		},
		"objectPosition": {
			"type": "string",
			"enum": ["top", "center", "bottom"],
			"default": "center"
		},
		"objectFit": {
			"type": "string",
			"enum": ["fill", "cover", "contain"],
			"default": "fill"
		},
		"useDefaultSrcset": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "yourtextdomain",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
